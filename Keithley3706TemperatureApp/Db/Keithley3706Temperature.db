# % macro, SYS,         System name
# % macro, DEV,         Device and axis number
# % macro, BUS,         Keithley you are talking to
# % macro, SLOT,        Slot number in keithley




record(ai, "$(SYS){$(DEV)-$(BUS)S$(SLOT)}Update")
{
  field (DESC, "Time")
  field (DTYP, "stream")
  field (SCAN, "10 second")
  field (INP, "@Keithley3706Temperature.proto ReadBuffer($(SYS),$(DEV),$(BUS),$(SLOT)) $(BUS)")
}


